# App
APP_NAME=FastAPI Template
DEBUG=True
ENVIRONMENT=development
VERSION=1.0.1

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/app_db
AUTO_CREATE_SCHEMA=false

# Server
HOST=0.0.0.0
PORT=8000
TIMEZONE=America/Argentina/Buenos_Aires

# Alembic
ALEMBIC_MIGRATION_PATH=migrations

# Logging
LOG_LEVEL=INFO
LOG_CONSOLE_ENABLED=true
AXIOM_LOGS_ENABLED=false
AXIOM_LOG_BATCH_SIZE=25
AXIOM_LOG_FLUSH_INTERVAL_SECONDS=2.0
AXIOM_REQUEST_TIMEOUT_SECONDS=5.0

# API
API_PREFIX=/api

# Axiom
AXIOM_API_KEY=YOUR_AXIOM_API_KEY
AXIOM_DATASET=testing_fastapi_template
AXIOM_TRACES_DATASET=
AXIOM_OTLP_ENDPOINT=https://api.axiom.co/v1/traces
AXIOM_BASE_URL=https://api.axiom.co
TRACING_ENABLED=false
TRACING_SAMPLE_RATIO=1.0